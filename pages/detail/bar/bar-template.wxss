.barContainer{
  display: flex;
  flex-direction: column;
}
.bar{
  display: flex;
  flex-direction: row;
  height: 15px;
  line-height: 15px;
  font-size: 12px;
  margin-bottom:15px;
  padding-left: 15%;
  padding-right: 15%;
}
.barTitle{
  width: 60rpx;
}
.barRate{
  height:15px;
  margin-left:5px;
  margin-right: 5px;
}
.barBackground{
  height: 15px;
  border-radius: 0 3px 3px 0;
}